<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MrMindedAdminLast" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#A182E8</item>
        <item name="android:navigationBarColor">#A182E8</item>
        <!-- Customize your theme here. -->
    </style>
    <style parent="ThemeOverlay.AppCompat.Dialog.Alert" name="TrackSelectionDialogThemeOverlay">
        <item name="windowNoTitle">false</item>
        <item name="windowActionBar">false</item>
        <item name="windowMinWidthMajor">@dimen/_100hsdp</item>
        <item name="android:windowMinWidthMinor">@dimen/_100hsdp</item>
        <item name="alpha">0.5</item>
    </style>
</resources>